# resolve links - $0 may be a softlink
PRG="$0"

while [ -h "$PRG" ]; do
  ls=`ls -ld "$PRG"`
  link=`expr "$ls" : '.*-> \(.*\)$'`
  if expr "$link" : '/.*' > /dev/null; then
    PRG="$link"
  else
    PRG=`dirname "$PRG"`/"$link"
  fi
done

PRGDIR=`dirname "$PRG"`
BASEDIR=`cd "$PRGDIR/.." >/dev/null; pwd`

# Reset the REPO variable. If you need to influence this use the environment setup file.
REPO=


# OS specific support.  $var _must_ be set to either true or false.
cygwin=false;
darwin=false;
case "`uname`" in
  CYGWIN*) cygwin=true ;;
  Darwin*) darwin=true
           if [ -z "$JAVA_VERSION" ] ; then
             JAVA_VERSION="CurrentJDK"
           else
             echo "Using Java version: $JAVA_VERSION"
           fi
		   if [ -z "$JAVA_HOME" ]; then
		      if [ -x "/usr/libexec/java_home" ]; then
			      JAVA_HOME=`/usr/libexec/java_home`
			  else
			      JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Versions/${JAVA_VERSION}/Home
			  fi
           fi       
           ;;
esac

if [ -z "$JAVA_HOME" ] ; then
  if [ -r /etc/gentoo-release ] ; then
    JAVA_HOME=`java-config --jre-home`
  fi
fi

# For Cygwin, ensure paths are in UNIX format before anything is touched
if $cygwin ; then
  [ -n "$JAVA_HOME" ] && JAVA_HOME=`cygpath --unix "$JAVA_HOME"`
  [ -n "$CLASSPATH" ] && CLASSPATH=`cygpath --path --unix "$CLASSPATH"`
fi

# If a specific java binary isn't specified search for the standard 'java' binary
if [ -z "$JAVACMD" ] ; then
  if [ -n "$JAVA_HOME"  ] ; then
    if [ -x "$JAVA_HOME/jre/sh/java" ] ; then
      # IBM's JDK on AIX uses strange locations for the executables
      JAVACMD="$JAVA_HOME/jre/sh/java"
    else
      JAVACMD="$JAVA_HOME/bin/java"
    fi
  else
    JAVACMD=`which java`
  fi
fi

if [ ! -x "$JAVACMD" ] ; then
  echo "Error: JAVA_HOME is not defined correctly." 1>&2
  echo "  We cannot execute $JAVACMD" 1>&2
  exit 1
fi

if [ -z "$REPO" ]
then
  REPO="$BASEDIR"/repo
fi

CLASSPATH="$BASEDIR"/etc:"$REPO"/com/google/code/gson/gson/2.8.5/gson-2.8.5.jar:"$REPO"/com/vk/api/sdk/1.0.14/sdk-1.0.14.jar:"$REPO"/org/slf4j/slf4j-api/1.7.26/slf4j-api-1.7.26.jar:"$REPO"/org/apache/commons/commons-text/1.6/commons-text-1.6.jar:"$REPO"/org/apache/commons/commons-lang3/3.6/commons-lang3-3.6.jar:"$REPO"/org/apache/commons/commons-collections4/4.3/commons-collections4-4.3.jar:"$REPO"/commons-io/commons-io/2.6/commons-io-2.6.jar:"$REPO"/org/asynchttpclient/async-http-client/2.8.1/async-http-client-2.8.1.jar:"$REPO"/org/asynchttpclient/async-http-client-netty-utils/2.8.1/async-http-client-netty-utils-2.8.1.jar:"$REPO"/io/netty/netty-buffer/4.1.33.Final/netty-buffer-4.1.33.Final.jar:"$REPO"/io/netty/netty-codec-http/4.1.33.Final/netty-codec-http-4.1.33.Final.jar:"$REPO"/io/netty/netty-common/4.1.33.Final/netty-common-4.1.33.Final.jar:"$REPO"/io/netty/netty-transport/4.1.33.Final/netty-transport-4.1.33.Final.jar:"$REPO"/io/netty/netty-codec/4.1.33.Final/netty-codec-4.1.33.Final.jar:"$REPO"/io/netty/netty-handler/4.1.33.Final/netty-handler-4.1.33.Final.jar:"$REPO"/io/netty/netty-codec-socks/4.1.33.Final/netty-codec-socks-4.1.33.Final.jar:"$REPO"/io/netty/netty-handler-proxy/4.1.33.Final/netty-handler-proxy-4.1.33.Final.jar:"$REPO"/io/netty/netty-transport-native-epoll/4.1.33.Final/netty-transport-native-epoll-4.1.33.Final-linux-x86_64.jar:"$REPO"/io/netty/netty-transport-native-unix-common/4.1.33.Final/netty-transport-native-unix-common-4.1.33.Final.jar:"$REPO"/io/netty/netty-resolver-dns/4.1.33.Final/netty-resolver-dns-4.1.33.Final.jar:"$REPO"/io/netty/netty-resolver/4.1.33.Final/netty-resolver-4.1.33.Final.jar:"$REPO"/io/netty/netty-codec-dns/4.1.33.Final/netty-codec-dns-4.1.33.Final.jar:"$REPO"/org/reactivestreams/reactive-streams/1.0.2/reactive-streams-1.0.2.jar:"$REPO"/com/typesafe/netty/netty-reactive-streams/2.0.0/netty-reactive-streams-2.0.0.jar:"$REPO"/com/sun/activation/javax.activation/1.2.0/javax.activation-1.2.0.jar:"$REPO"/org/apache/httpcomponents/httpclient/4.5.8/httpclient-4.5.8.jar:"$REPO"/org/apache/httpcomponents/httpcore/4.4.11/httpcore-4.4.11.jar:"$REPO"/commons-logging/commons-logging/1.2/commons-logging-1.2.jar:"$REPO"/commons-codec/commons-codec/1.11/commons-codec-1.11.jar:"$REPO"/org/apache/httpcomponents/httpmime/4.5.8/httpmime-4.5.8.jar:"$REPO"/org/apache/logging/log4j/log4j-slf4j-impl/2.7/log4j-slf4j-impl-2.7.jar:"$REPO"/org/apache/logging/log4j/log4j-api/2.7/log4j-api-2.7.jar:"$REPO"/org/apache/logging/log4j/log4j-core/2.7/log4j-core-2.7.jar:"$REPO"/com/googlecode/json-simple/json-simple/1.1.1/json-simple-1.1.1.jar:"$REPO"/junit/junit/4.10/junit-4.10.jar:"$REPO"/org/hamcrest/hamcrest-core/1.1/hamcrest-core-1.1.jar:"$REPO"/org/jsoup/jsoup/1.12.1/jsoup-1.12.1.jar:"$REPO"/org/mariadb/jdbc/mariadb-java-client/3.1.2/mariadb-java-client-3.1.2.jar:"$REPO"/com/github/waffle/waffle-jna/3.2.0/waffle-jna-3.2.0.jar:"$REPO"/net/java/dev/jna/jna/5.12.1/jna-5.12.1.jar:"$REPO"/net/java/dev/jna/jna-platform/5.12.1/jna-platform-5.12.1.jar:"$REPO"/org/slf4j/jcl-over-slf4j/1.7.36/jcl-over-slf4j-1.7.36.jar:"$REPO"/com/github/ben-manes/caffeine/caffeine/2.9.3/caffeine-2.9.3.jar:"$REPO"/com/google/errorprone/error_prone_annotations/2.10.0/error_prone_annotations-2.10.0.jar:"$REPO"/org/checkerframework/checker-qual/3.23.0/checker-qual-3.23.0.jar:"$REPO"/zow1e2k/VKBOT/1.0-SNAPSHOT/VKBOT-1.0-SNAPSHOT.jar

ENDORSED_DIR=
if [ -n "$ENDORSED_DIR" ] ; then
  CLASSPATH=$BASEDIR/$ENDORSED_DIR/*:$CLASSPATH
fi

if [ -n "$CLASSPATH_PREFIX" ] ; then
  CLASSPATH=$CLASSPATH_PREFIX:$CLASSPATH
fi

# For Cygwin, switch paths to Windows format before running java
if $cygwin; then
  [ -n "$CLASSPATH" ] && CLASSPATH=`cygpath --path --windows "$CLASSPATH"`
  [ -n "$JAVA_HOME" ] && JAVA_HOME=`cygpath --path --windows "$JAVA_HOME"`
  [ -n "$HOME" ] && HOME=`cygpath --path --windows "$HOME"`
  [ -n "$BASEDIR" ] && BASEDIR=`cygpath --path --windows "$BASEDIR"`
  [ -n "$REPO" ] && REPO=`cygpath --path --windows "$REPO"`
fi

exec "$JAVACMD" $JAVA_OPTS  \
  -classpath "$CLASSPATH" \
  -Dapp.name="Main" \
  -Dapp.pid="$$" \
  -Dapp.repo="$REPO" \
  -Dapp.home="$BASEDIR" \
  -Dbasedir="$BASEDIR" \
  Main \
  "$@"
